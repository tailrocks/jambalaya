{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 20,
  "packageRules": [
    {
      "allowedVersions": "/^[0-9]+\\.[0-9]+(\\.[0-9]+)?$/",
      "matchPackageNames": [
        "com.graphql-java:{/,}**"
      ]
    }
  ]
}
